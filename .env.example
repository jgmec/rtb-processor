# SFTP Settings
SFTP_HOST=your-sftp-server
SFTP_PORT=22
SFTP_USER=sftpuser
SFTP_PASSWORD=your-password
SFTP_REMOTE_DIR=/upload/zips
SFTP_POLL_INTERVAL_SECONDS=30

# Local Storage
LOCAL_DOWNLOAD_DIR=/tmp/rtb-downloads

# ClickHouse Settings
CLICKHOUSE_HOST=clickhouse
CLICKHOUSE_PORT=9000
CLICKHOUSE_DB=default
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=
CLICKHOUSE_BATCH_SIZE=1000

# RabbitMQ Settings
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672/
RABBITMQ_QUEUE=rtb_files

# Processing Settings
MAX_WORKERS=4
